{
  "name": "pmt-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "bootstrap": "^4.1.3",
    "history": "^4.7.2",
    "http-proxy-middleware": "^0.19.1",
    "jquery": "^3.3.1",
    "react-redux": "^6.0.0",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
